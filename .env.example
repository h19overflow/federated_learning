# Database Configuration
POSTGRES_DB=your_database_name
POSTGRES_USER=your_database_user
POSTGRES_PASSWORD=your_database_password
POSTGRES_PORT=5432
POSTGRES_DB_URI=postgresql://your_user:your_password@localhost:5432/your_database

# API Configuration
API_BASE_URL=http://localhost
API_VERSION=v1
API_PREFIX=/api

# WebSocket Configuration
WEBSOCKET_HOST=localhost
WEBSOCKET_PORT=8765

# CORS Configuration
# Can be set as JSON array or comma-separated list
CORS_ORIGINS=["http://localhost:5173","http://localhost:8080","http://localhost:8081","http://127.0.0.1:5173","http://127.0.0.1:8080","http://127.0.0.1:8081"]

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Federated Learning Configuration
FL_NUM_ROUNDS=3
FL_NUM_CLIENTS=2
FL_FRACTION_FIT=1.0
FL_FRACTION_EVALUATE=1.0

# AI/LLM Configuration
GEMINI_API_KEY=your_gemini_api_key
GOOGLE_API_KEY=your_google_api_key
BASE_LLM=gemini-pro

# LangSmith Configuration
LANGSMITH_TRACING=false
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT=your_project_name

# Paths (optional, can use defaults)
# DATA_DIR=./data
# RESULTS_DIR=./results
# CHECKPOINT_DIR=./results/checkpoints
